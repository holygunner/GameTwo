<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/start_button_corners"/>
            <stroke android:width="1dp" android:color="#fcffe2" />
            <gradient android:angle="-90" android:startColor="#ffffff" android:endColor="#a09f8e" />
        </shape>
    </item>

    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/start_button_corners"/>
            <stroke android:width="0dp" android:color="#e4e7c5" />
            <gradient android:angle="-90" android:startColor="#f9faf5" android:endColor="#69685d" />
        </shape>
    </item>
</selector>